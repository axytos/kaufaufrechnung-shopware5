<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <!-- set log level to INFO
             possible values:
                DEBUG = 100
                INFO = 200
                NOTICE = 250
                WARNING = 300
                ERROR = 400
                CRITICAL = 500
                ALERT = 550
                EMERGENCY = 600
        -->
        <parameter key="axytos_kauf_auf_rechnung_shopware5.logger.level">200</parameter>
    </parameters>

    <services>
        <!-- Logger Integration ========================================================================================================= -->

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Logging\LoggerAdapter" class="AxytosKaufAufRechnungShopware5\Logging\LoggerAdapter" />
        <service id="Axytos\ECommerce\Logging\LoggerAdapterInterface" alias="AxytosKaufAufRechnungShopware5\Logging\LoggerAdapter" public="true"/>

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ErrorReporting\ErrorHandler" class="AxytosKaufAufRechnungShopware5\ErrorReporting\ErrorHandler" public="true"/>

        <!-- Configuration Integration ================================================================================================== -->

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Configuration\PluginConfiguration" class="AxytosKaufAufRechnungShopware5\Configuration\PluginConfiguration" public="true"/>

        <!-- ECommerce Client Integration =============================================================================================== -->

        <service
            autowire="true"
            id="Axytos\ECommerce\Clients\Invoice\PluginConfigurationValidator"
            class="AxytosKaufAufRechnungShopware5\Subscriber\PluginConfigurationValidation\PluginConfigurationValidatorDecorator" public="true"/>

        <service autowire="true" id="Axytos\ECommerce\PackageInfo\ComposerPackageInfoProvider" class="Axytos\ECommerce\PackageInfo\ComposerPackageInfoProvider"/>

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Client\ApiHostProvider" class="AxytosKaufAufRechnungShopware5\Client\ApiHostProvider"/>
        <service id="Axytos\ECommerce\Abstractions\ApiHostProviderInterface" alias="AxytosKaufAufRechnungShopware5\Client\ApiHostProvider" public="true"/>

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Client\ApiKeyProvider" class="AxytosKaufAufRechnungShopware5\Client\ApiKeyProvider"/>
        <service id="Axytos\ECommerce\Abstractions\ApiKeyProviderInterface" alias="AxytosKaufAufRechnungShopware5\Client\ApiKeyProvider" public="true"/>

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Client\FallbackModeConfiguration" class="AxytosKaufAufRechnungShopware5\Client\FallbackModeConfiguration"/>
        <service id="Axytos\ECommerce\Abstractions\FallbackModeConfigurationInterface" alias="AxytosKaufAufRechnungShopware5\Client\FallbackModeConfiguration" public="true"/>

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Client\PaymentMethodConfiguration" class="AxytosKaufAufRechnungShopware5\Client\PaymentMethodConfiguration"/>
        <service id="Axytos\ECommerce\Abstractions\PaymentMethodConfigurationInterface" alias="AxytosKaufAufRechnungShopware5\Client\PaymentMethodConfiguration" public="true"/>

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Client\UserAgentInfoProvider" class="AxytosKaufAufRechnungShopware5\Client\UserAgentInfoProvider"/>
        <service id="Axytos\ECommerce\Abstractions\UserAgentInfoProviderInterface" alias="AxytosKaufAufRechnungShopware5\Client\UserAgentInfoProvider" public="true"/>

        <service autowire="true" id="Axytos\ECommerce\AxytosECommerceClient" class="Axytos\ECommerce\AxytosECommerceClient"/>
        <service id="Axytos\ECommerce\Clients\ErrorReporting\ErrorReportingClientInterface" alias="Axytos\ECommerce\AxytosECommerceClient" public="true"/>
        <service id="Axytos\ECommerce\Clients\Checkout\CheckoutClientInterface" alias="Axytos\ECommerce\AxytosECommerceClient" public="true"/>
        <service id="Axytos\ECommerce\Clients\CredentialValidation\CredentialValidationClientInterface" alias="Axytos\ECommerce\AxytosECommerceClient" public="true"/>
        <service id="Axytos\ECommerce\Clients\Invoice\InvoiceClientInterface" alias="Axytos\ECommerce\AxytosECommerceClient" public="true"/>

        <service autowire="true" id="Axytos\ECommerce\DataMapping\DtoToDtoMapper" class="Axytos\ECommerce\DataMapping\DtoToDtoMapper"/>

        <!-- Core Integration =========================================================================================================== -->
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Core\InvoiceOrderContextFactory" class="AxytosKaufAufRechnungShopware5\Core\InvoiceOrderContextFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Core\OrderStateMachine" class="AxytosKaufAufRechnungShopware5\Core\OrderStateMachine" public="true"/>
        <service id="AxytosKaufAufRechnungShopware5\Core\OrderCheckProcessStateMachine" class="AxytosKaufAufRechnungShopware5\Core\OrderCheckProcessStateMachine" public="true"/>

        <!-- DataAbstractionLayer Integration =========================================================================================== -->

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataAbstractionLayer\ArticleDetailRepository" class="AxytosKaufAufRechnungShopware5\DataAbstractionLayer\ArticleDetailRepository" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataAbstractionLayer\DispatchRepository" class="AxytosKaufAufRechnungShopware5\DataAbstractionLayer\DispatchRepository" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataAbstractionLayer\OrderRepository" class="AxytosKaufAufRechnungShopware5\DataAbstractionLayer\OrderRepository" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataAbstractionLayer\OrderAttributesRepository" class="AxytosKaufAufRechnungShopware5\DataAbstractionLayer\OrderAttributesRepository" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataAbstractionLayer\LegacyOrderAttributesRepository" class="AxytosKaufAufRechnungShopware5\DataAbstractionLayer\LegacyOrderAttributesRepository" public="true"/>

        <!-- DataMapping Integration ==================================================================================================== -->

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\DeliveryWeightCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\DeliveryWeightCalculator" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\LogisticianCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\LogisticianCalculator" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionGrossPriceCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionGrossPriceCalculator" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionGrossPricePerUnitCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionGrossPricePerUnitCalculator" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionNetPriceCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionNetPriceCalculator" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionNetPricePerUnitCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionNetPricePerUnitCalculator" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionProductIdCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionProductIdCalculator" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionProductNameCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionProductNameCalculator" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionQuantityCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionQuantityCalculator" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionTaxCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionTaxCalculator" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionTaxPercentCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\PositionTaxPercentCalculator" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\ValueCalculation\TrackingIdCalculator" class="AxytosKaufAufRechnungShopware5\ValueCalculation\TrackingIdCalculator" public="true"/>

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\BasketDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\BasketDtoFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\BasketPositionDtoCollectionFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\BasketPositionDtoCollectionFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\BasketPositionDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\BasketPositionDtoFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\CustomerDataDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\CustomerDataDtoFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\DeliveryAddressDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\DeliveryAddressDtoFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\InvoiceAddressDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\InvoiceAddressDtoFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\CreateInvoiceBasketDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\CreateInvoiceBasketDtoFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\CreateInvoiceBasketPositionDtoCollectionFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\CreateInvoiceBasketPositionDtoCollectionFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\CreateInvoiceBasketPositionDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\CreateInvoiceBasketPositionDtoFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\CreateInvoiceTaxGroupDtoCollectionFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\CreateInvoiceTaxGroupDtoCollectionFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\CreateInvoiceTaxGroupDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\CreateInvoiceTaxGroupDtoFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\RefundBasketDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\RefundBasketDtoFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\RefundBasketPositionDtoCollectionFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\RefundBasketPositionDtoCollectionFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\RefundBasketPositionDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\RefundBasketPositionDtoFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\RefundBasketTaxGroupDtoCollectionFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\RefundBasketTaxGroupDtoCollectionFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\RefundBasketTaxGroupDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\RefundBasketTaxGroupDtoFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\ShippingBasketPositionDtoCollectionFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\ShippingBasketPositionDtoCollectionFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\DataMapping\ShippingBasketPositionDtoFactory" class="AxytosKaufAufRechnungShopware5\DataMapping\ShippingBasketPositionDtoFactory" public="true"/>

        <!-- Event Subscribers ========================================================================================================== -->

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Subscriber\CheckoutSubscriber" class="AxytosKaufAufRechnungShopware5\Subscriber\CheckoutSubscriber" public="true">
            <tag name="shopware.event_subscriber"/>
        </service>

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Subscriber\PluginConfigurationValidation\PaymentMethodConfigurationPreUpdateSubscriber" class="AxytosKaufAufRechnungShopware5\Subscriber\PluginConfigurationValidation\PaymentMethodConfigurationPreUpdateSubscriber" public="true">
            <tag name="doctrine.event_subscriber"/>
        </service>

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Subscriber\PluginConfigurationValidation\PluginConfigurationPostUpdateSubscriber" class="AxytosKaufAufRechnungShopware5\Subscriber\PluginConfigurationValidation\PluginConfigurationPostUpdateSubscriber" public="true">
            <tag name="doctrine.event_subscriber"/>
        </service>

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\Subscriber\PluginConfigurationValidation\DispatchPostUpdateSubscriber" class="AxytosKaufAufRechnungShopware5\Subscriber\PluginConfigurationValidation\DispatchPostUpdateSubscriber" public="true">
            <tag name="doctrine.event_subscriber"/>
        </service>

        <!-- Cron Job ========================================================================================================== -->

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\CronJob\ShopSystemOrderFactory" class="AxytosKaufAufRechnungShopware5\CronJob\ShopSystemOrderFactory" public="true"/>
        <service autowire="true" id="AxytosKaufAufRechnungShopware5\CronJob\ShopSystemOrderRepository" class="AxytosKaufAufRechnungShopware5\CronJob\ShopSystemOrderRepository" public="true"/>
        <service id="Axytos\ECommerce\OrderSync\ShopSystemOrderRepositoryInterface" alias="AxytosKaufAufRechnungShopware5\CronJob\ShopSystemOrderRepository" public="true"/>

        <service autowire="true" id="Axytos\ECommerce\OrderSync\OrderSyncItemFactory" class="Axytos\ECommerce\OrderSync\OrderSyncItemFactory" public="true"/>
        <service autowire="true" id="Axytos\ECommerce\OrderSync\OrderSyncItemRepository" class="Axytos\ECommerce\OrderSync\OrderSyncItemRepository" public="true"/>
        <service autowire="true" id="Axytos\ECommerce\OrderSync\OrderSyncWorker" class="Axytos\ECommerce\OrderSync\OrderSyncWorker" public="true"/>

        <service autowire="true" id="AxytosKaufAufRechnungShopware5\CronJob\OrderSyncCronJobSubscriber" class="AxytosKaufAufRechnungShopware5\CronJob\OrderSyncCronJobSubscriber" public="true">
            <tag name="shopware.event_subscriber"/>
        </service>

    </services>
</container>
